
public class Main{
    public static void main(String[] args) {
      int a[]= {3,5,4,1,9};
      int max=a[0];
      int min=a[0];
      for(int i=1;i<a.length;i++) {
    	  if(max<a[i])
    		  max=a[i];
    	  if(min>a[i])
    		  min=a[i];
    	  
      }
     System.out.println("Maximum value: "+max);
     System.out.println("Minimum value: "+min);
    }
}
