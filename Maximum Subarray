public class Main {
	
	public static void main(String[] args) {
		int a[]= {2,3,-8,7,-1,2,3};
    int result=0;
		for(int i=0;i<a.length;i++) {
			int sum=0;
			for(int j=i;j<a.length;j++) {
				sum=sum+a[j];
				result=Math.max(result, sum);
			}
		}
		System.out.println("Maximum sum of subarray: "+result);
	
	}
}
